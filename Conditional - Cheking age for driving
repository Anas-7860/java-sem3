import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner s = new Scanner(System.in);
      int age = s.nextInt();
      if(age >=18 &&age<=65){
        System.out.println("you are eligible for driving !");
      }
      else if(age<=0){
        System.out.println("Invalid input ! Age cant be less than 0");
      }
      else if(age>=1 && age<=12){
        System.out.println("you ARE A CHILD And not eligble");
      }
      else System.out.println("You are not eligible !");
  }
}
